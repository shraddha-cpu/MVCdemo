
@{
    ViewData["Title"] = "List";
}

<h1>Employee List</h1>
<table class="table table-bordered">
   <tr>
       <th>Emp Id</th>
       <th>Emp Name</th>
       <th>Designation</th>
       <th>Salary</th>
   </tr>
   @foreach(Employee e in (IEnumerable<Employee>)ViewData["emplist"])
       {
           <tr>
                <td>@e.Id</td>
                <td>@e.Name</td>
                <td>@e.Designation</td>
                <td>@e.Salary</td>
           </tr>
       }
</table>
<h3>Using View Bag</h3>
<table class="table table-bordered"
<tr>
    <th>Emp Id</th>
    <th>Emp Name</th>
       <th>Designation</th>
       <th>Salary</th>
   </tr>
    @foreach (Employee e in ViewBag.empList)
    {
               <tr>
                    <td>@e.Id</td>
                    <td>@e.Name</td>
                    <td>@e.Designation</td>
                    <td>@e.Salary</td>
               </tr>
    }
    </table>



